# Dimension line spacing set to 20.

# Contiguous right lines.
line x300;y300, w200
dimen &^[0], &^[1], x300;y320, x500;y320
line &^[1], h-100
dimen &^[0], &^[1], x520;y300, x520;y200
line $^[1], w-200
dimen &^[0], &^[1], x500;y180, x300;y180
line $^[1], h100
dimen &^[0], &^[1], x280;y200, x280;y300

# 45deg lines.
line x700;y400, x800;y300
dimen &^[0], &^[1], x720;y420, x820;y320 # -\
line &^[1], x700;y200
dimen &^[0], &^[1], x820;y280, x720;y180 # -/
line x700;y400, x600;y300
dimen &^[0], &^[1], x680;y420, x580;y320 # _\ wrong
line &^[1], x700;y200
dimen &^[0], &^[1], x580;y280, x680;y180 # _/ wrong

